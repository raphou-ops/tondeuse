            hideMinMax: true
        });

        var g7 = new JustGage({
            id: "ValveEF",
            value: '<%= valveEF %>',
            min: 0,
            max: 100,
            shadow: true,
            pointer: true,
            pointerOptions: {
                toplength: 6,
                bottomlength: 6,
                bottomwidth: 6,
                stroke: 'none',
                stroke_width: 0,
                stroke_linecap: 'square',
                color: '#aa0404'
